{
  "name": "@nativescript-community/svelte-native",
  "version": "1.0.29",
  "description": "Svelte integration for NativeScript",
  "sideEffects": false,
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nativescript-community/svelte-native.git"
  },
  "homepage": "https://github.com/nativescript-community/svelte-native",
  "keywords": [
    "nativescript",
    "svelte"
  ],
  "scripts": {
    "release": "standard-version",
    "clean": "npx rimraf ./dist/*",
    "build": "npm run clean && rollup -c && node ./scripts/create-pkg.js",
    "watch": "rollup -cw",
    "deploy": "npm run build && cd dist && npm publish",
    "deploy-beta": "npm run build && cd dist && npm publish --tag beta"
  },
  "author": "David Pershouse",
  "license": "MIT",
  "peerDependencies": {
    "@nativescript/core": "8.9.9",
    "svelte": "4.2.20"
  },
  "devDependencies": {
    "@nativescript/core": "8.9.9",
    "@nativescript/types": "8.9.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/node": "20.14.9",
    "glob": "10.3.10",
    "rimraf": "2.7.1",
    "rollup": "4.18.0",
    "rollup-plugin-svelte": "^7.2.2",
    "standard-version": "9.5.0",
    "svelte": "4.2.20",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  }
}
